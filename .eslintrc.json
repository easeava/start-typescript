{
	"env": {
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": ["standard", "standard-typescript-prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": 0
	}
}
